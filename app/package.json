{
  "name": "karapaia_matome_api",
  "version": "1.0.0",
  "description": "カラパイアまとめのバックエンド",
  "main": "serverless.ts",
  "scripts": {
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "local:invoke": "sls offline start",
    "local:savePosts": "npx ts-node src/sample/save-posts.ts",
    "local:deletePosts": "npx ts-node src/sample/delete-posts.ts",
    "lint": "eslint --fix **/*.{js,ts}",
    "format": "prettier --write **/*.{js,ts}",
    "migrate:up": "npx ts-node src/dynamodb/migrations/create-karapaia-matome-table.ts",
    "migrate:down": "npx ts-node src/dynamodb/migrations/drop-karapaia-matome-table.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "aws-sdk": "^2.963.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.81",
    "@types/chardet": "^0.8.1",
    "@types/iconv-lite": "^0.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "aws-lambda": "^1.0.6",
    "chardet": "^1.3.0",
    "eslint": "^7.32.0",
    "iconv-lite": "^0.6.3",
    "jsdom": "^16.7.0",
    "json-schema-to-ts": "^1.5.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "serverless": "^2.23.0",
    "serverless-offline": "^8.0.0",
    "serverless-webpack": "^5.3.5",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2"
  },
  "author": "Yota-K",
  "license": "MIT"
}
